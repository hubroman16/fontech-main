{
  "RabbitMqSettings":{
    "QueueName" : "queue",
    "RoutingKey": "fontechproject.topic", 
    "ExchangeName": "fontechproject.exchange"
},
  "Serilog": {
    "Using": ["Serilog.Sinks.File", "Serilog.Sinks.Console"],
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [{
      "Name": "Console"
    },
      {
        "Name": "File",
        "Args": {
          "path": "../FontechProject.Api/logs/log-.txt",
          "rollOnFileSizeLimit": true,
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter,Serilog.Formatting.Compact",
          "rollingInterval" : "Day"
        }
    }],
      "Enrich": ["FromLogContext", "WithThreadId", "WithMachineName"]
  },
  "JWT": {
    "Issuer": "FontechProject",
    "Audience": "FontechProject",
    "Authority": "FontechProject",
    "JwtKey": "VDdYF0TsFr2zAIMuNAzEgIDxaEXu07bm",
    "Lifetime": 15,
    "RefreshTokenValidityInDays": 7
  },
  "AllowedHosts": "*"
}
